{
  "manifest_version": 2,
  "name": "show me the total airbnb trip price",
  "description": "Show the total price of your airbnb trip on search result pages",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "https://www.airbnb.com/s/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.airbnb.com/s/*"],
      "js": ["myscript.js"],
      "run_at": "document_end"
    }
  ]
}
